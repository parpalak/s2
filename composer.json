{
    "config": {
        "vendor-dir": "_vendor",
        "preferred-install": {
            "s2/rose": "src"
        }
    },
    "name": "s2/cms",
    "description": "Small and fast CMS",
    "minimum-stability": "stable",
    "license": "GPL",
    "authors": [
        {
            "name": "Roman Parpalak",
            "email": "roman@parpalak.com"
        }
    ],
    "require": {
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "altorouter/altorouter": "dev-master",
        "psr/log": "^1.1",
        "johnykvsky/jklogger": "^0.1.2",
        "s2/rose": "dev-master",
        "symfony/error-handler": "^7.0",
        "symfony/cache": "^7.0",
        "symfony/filesystem": "^7.0",
        "matthiasmullie/minify": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "S2\\Cms\\": "_include/src"
        }
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0.0",
        "codeception/module-phpbrowser": "^3.0"
    }
}
